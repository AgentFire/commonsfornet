      <dependency id="Commons.Utils" version="0.1.1.16" />
      <dependency id="Commons.Collections" version="0.1.1.16" />
    </dependencies>
  </metadata>
  <files>
      <file src="src\bin\Commons.Json.dll" target="lib\net45\" />
  </files>
</package>
